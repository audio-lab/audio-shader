    "GPU sound",
    "processing",
    "gl",
    "glsl",
    "nogl"
  ],
  "author": "Dima Yvanow <dfcreative@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/audio-lab/audio-shader/issues"
  },
  "homepage": "https://github.com/audio-lab/audio-shader#readme",
  "dependencies": {
    "brfs": "^1.4.3",
    "gl-shader": "^4.2.0",
    "gl-shader-output": "^2.0.1",
    "inherits": "^2.0.1",
    "nogl": "^1.1.0",
    "nogl-shader-output": "^1.4.0",
    "webgl-context": "^2.2.0",
    "xtend": "^4.0.1"
  },
  "devDependencies": {
