{
  "name": "audio-shader",
  "version": "1.2.3",
  "description": "Shader based audio processing stream",
  "main": "index.js",
  "scripts": {
    "test": "node test/index.js",
    "test:browser": "budo test/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/audio-lab/audio-shader.git"
